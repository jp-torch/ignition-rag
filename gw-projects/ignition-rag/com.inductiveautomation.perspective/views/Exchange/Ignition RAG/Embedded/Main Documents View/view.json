            "events": {
              "component": {
                "onFileReceived": {
                  "config": {
                    "script": "\tself.getSibling(\"Upload In Progress\").position.display \u003d True\n\tfileName \u003d event.file.name\n\tfileBytes \u003d event.file.getBytes()\n\t\n\tdocumentId \u003d exchange.ignitionRAG.utils.getId()\n\ttx \u003d system.db.beginTransaction(\"ignition-rag\")\n\t\n\tparams \u003d {\n\t\t\"id\": documentId,\n\t\t\"bytes\": fileBytes,\n\t\t\"source\": fileName,\n\t\t\"userId\": self.session.props.auth.user.id if self.session.props.auth.user.id else \"\"\n\t}\n\t\n\ttry:\n\t\tsystem.db.execUpdate(\"Exchange/Ignition RAG/Insert Document\", params, tx\u003dtx)\n\t\texchange.ignitionRAG.rag.loadData(documentId, fileBytes, fileName, tx)\n\t\tsystem.perspective.openPopup(\"ignition-rag-message\", \"Exchange/Ignition RAG/Popups/Message\", {\"message\": \"%s uploaded successfully!\" % fileName})\n\t\tsystem.db.commitTransaction(tx)\n\texcept Exception as e:\n\t\tsystem.db.rollbackTransaction(tx)\n\t\tsystem.perspective.openPopup(\"ignition-rag-message\", \"Exchange/Ignition RAG/Popups/Message\", {\"message\": \"Error uploading %s: %s\" % (fileName, str(e))})\n\tfinally:\n\t\tsystem.db.closeTransaction(tx)\n\t\tsystem.perspective.sendMessage(\"ignition-rag-update-docs\", {})\n\t\tself.getSibling(\"Upload In Progress\").position.display \u003d False"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
